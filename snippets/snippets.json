{
  "pa": {
    "description": "[pa]ir",
    "prefix": "pa",
    "body": [
      "${1:key}: ${2:value}"
    ]
  },
  "in": {
    "description": "[in]cluded multilines",
    "prefix": "in",
    "body": [
      "${1:key}: |",
      "\t${2:value}"
    ]
  },
  "fo": {
    "description": "[fo]lded multilines",
    "prefix": "fo",
    "body": [
      "${1:key}: >",
      "\t${2:value}"
    ]
  },
  "if": {
    "description": "[if]",
    "prefix": "if",
    "body": [
      "\"if\": {",
      "\t\"properties\": {",
      "\t\t\"${1:property1}\": {",
      "\t\t\t\"${2|const,pattern|}\": ${3:value}",
      "\t\t}",
      "\t}",
      "},",
      "\"then\": {",
      "\t\"properties\": {",
      "\t\t\"${4:property2}\": {",
      "\t\t\t${5:requirenments}",
      "\t\t}",
      "\t}",
      "},"
    ]
  }
}